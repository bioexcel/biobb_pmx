{
    "_id": "biobb_pmx",
    "desc": "Biobb_pmx is the Biobb module collection to perform PMX executions",
    "github": "https://github.com/bioexcel/biobb_pmx",
    "readthedocs": "https://biobb-pmx.readthedocs.io/en/latest/",
    "conda": "https://anaconda.org/bioconda/biobb_pmx",
    "docker": "https://quay.io/biocontainers/biobb_pmx:5.0.0--pyhdfd78af_0",
    "singularity": "https://depot.galaxyproject.org/singularity/biobb_pmx:5.0.0--pyhdfd78af_0",
    "version": "5.0.0",
    "rest": true,
    "tools": [
        {
            "block": "Pmxmutate",
            "tool": "pmx",
            "desc": "Wrapper class for the PMX mutate module.",
            "exec": "pmxmutate",
            "docs": "https://biobb-pmx.readthedocs.io/en/latest/pmx.html#module-pmx.pmxmutate",
            "rest": true
        },
        {
            "block": "PmxGentop",
            "tool": "pmx",
            "desc": "Wrapper class for the PMX gentop module.",
            "exec": "pmxgentop",
            "docs": "https://biobb-pmx.readthedocs.io/en/latest/pmx.html#module-pmx.pmxgentop",
            "rest": true
        },
        {
            "block": "Pmxanalyse",
            "tool": "pmx",
            "desc": "Wrapper class for the PMX analyse module.",
            "exec": "pmxanalyse",
            "docs": "https://biobb-pmx.readthedocs.io/en/latest/pmx.html#module-pmx.pmxanalyse",
            "rest": true
        },
        {
            "block": "Pmxatom_mapping",
            "tool": "pmx",
            "desc": "Wrapper class for the PMX atom mapping module.",
            "exec": "pmxatom_mapping",
            "docs": "https://biobb-pmx.readthedocs.io/en/latest/pmx.html#module-pmx.pmxatom_mapping",
            "rest": true
        },
        {
            "block": "Pmxcreate_top",
            "tool": "pmx",
            "desc": "Wrapper class for the PMX create topology module.",
            "exec": "pmxcreate_top",
            "docs": "https://biobb-pmx.readthedocs.io/en/latest/pmx.html#module-pmx.pmxcreate_top",
            "rest": true
        },
        {
            "block": "Pmxligand_hybrid",
            "tool": "pmx",
            "desc": "Wrapper class for the PMX hybrid ligand module.",
            "exec": "pmxligand_hybrid",
            "docs": "https://biobb-pmx.readthedocs.io/en/latest/pmx.html#module-pmx.pmxligand_hybrid",
            "rest": true
        },
        {
            "block": "Pmxmerge_ff",
            "tool": "pmx",
            "desc": "Wrapper class for the PMX merge forcefield module.",
            "exec": "pmxmerge_ff",
            "docs": "https://biobb-pmx.readthedocs.io/en/latest/pmx.html#module-pmx.pmxmerge_ff",
            "rest": true
        }
    ],
    "dep_pypi": [
        "install_requires=['biobb_common==5.0.0']",
        "python_requires='>=3.9'"
    ],
    "dep_conda": [
        "python >=3.9",
        "biobb_common ==5.0.0",
        "pmx_biobb ==4.1.3"
    ],
    "keywords": [
        "pmx",
        "energy"
    ]
}